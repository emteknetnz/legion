{
    "name": "emteknetnz/legion",
    "type": "library",
    "description": "Parallel silverstripe testing using docker",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/framework": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Emteknetnz\\Legion\\": "src/",
            "Emteknetnz\\Legion\\Tests\\": "tests/"
        },
        "files": [
            "dockerenv/bootstrap.php"
        ]
    },
    "bin": [
        "legion"
    ],
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
